//	      	dba.receiver.id_receiver < 900000  ;
	
	SELECT ID_RECEIVER		INTO	:ld_ultimo	FROM dba.BRANCH_LAST_TRANSACTION	WHERE ID_BRANCH = :ls_branch;
	//**********************************************************************************************
	
end if			



If isnull(ld_ultimo) Then
	ld_ultimo = 0
END IF

Return ld_ultimo


end function

