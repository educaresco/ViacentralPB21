 retrieve="SELECT  DBA.RECEIVER.ID_RECEIVER ,                DBA.RECEIVER.NET_AMOUNT_RECEIVER ,           DBA.TRANSACCION_DIARIA_PAYEE.CONS_TRANS_DIARIA ,           DBA.TRANSACCION_DIARIA_PAYEE.ID_GROUP_TRANS_DIARIA ,           DBA.TRANSACCION_DIARIA_PAYEE.DATE_TRANS_DIARIA ,           DBA.TRANSACCION_DIARIA_PAYEE.HOUR_TRANS_DIARIA ,           DBA.TRANSACCION_DIARIA_PAYEE.ID_CONCEPTO_CONTABLE ,           DBA.TRANSACCION_DIARIA_PAYEE.DES_TRANS_DIARIA ,           DBA.TRANSACCION_DIARIA_PAYEE.CREDIT_TRANS_DIARIA ,           DBA.RECEIVER.COMMISSION_PAYEE ,           DBA.RELACION_INVOICES_PAYEE.ID_BRANCH_INVOICE ,           DBA.TRANSACCION_DIARIA_PAYEE.DEBIT_TRANS_DIARIA ,           DBA.RECEIVER.HANDLING_RECEIVER ,           DBA.RECEIVER.ID_SENDER ,           DBA.RECEIVER.ID_BRANCH ,                 DBA.RECEIVER.ID_FLAG_RECEIVER ,                     DBA.RECEIVER.ZIP_RECEIVER ,           DBA.RECEIVER.NOTES_RECEIVER ,           DBA.RECEIVER.DATE_RECEIVER ,           DBA.RECEIVER.TIME_RECEIVER ,           DBA.RECEIVER.RATE_CHANGE_RECEIVER ,           DBA.RECEIVER.TELEX_RECEIVER ,           DBA.RECEIVER.URGENCY_RECEIVER ,           DBA.RECEIVER.RECOMEND_RECEIVER ,        DBA.RECEIVER.EXCHANGE_RECEIVER ,           DBA.RECEIVER.TOTAL_RECEIVER ,             DBA.RECEIVER.TOTAL_PAY_RECEIVER ,           DBA.RECEIVER.TRANS_RECEIVER ,           DBA.RECEIVER.EXCHANGE_COMPANY ,           DBA.RECEIVER.TELEX_COMPANY ,           DBA.RECEIVER.REF_RECEIVER ,               DBA.RECEIVER.TOTAL_DIFERENCE ,           DBA.RECEIVER.TOTAL_MODO_PAGO ,           DBA.RECEIVER.TOTAL_MODO_PAGO_COMP ,               DBA.RECEIVER.STATUS_PAGO_PAYEE ,           DBA.RECEIVER.STATUS_PAGO_AGENT ,                    DBA.RECEIVER.FEE_RATE  
        FROM DBA.RECEIVER ,           DBA.RELACION_INVOICES_PAYEE ,           DBA.TRANSACCION_DIARIA_PAYEE     
        WHERE ( DBA.RECEIVER.ID_RECEIVER = DBA.RELACION_INVOICES_PAYEE.ID_RECEIVER ) and          ( DBA.TRANSACCION_DIARIA_PAYEE.CONS_TRANS_DIARIA = DBA.RELACION_INVOICES_PAYEE.CONS_TRANS_DIARIA ) and          ( DBA.RELACION_INVOICES_PAYEE.ID_BRANCH_INVOICE = DBA.RECEIVER.ID_BRANCH ) and     ( DBA.TRANSACCION_DIARIA_PAYEE.ID_CONCEPTO_CONTABLE IN ('TD','CC')) AND      ( DBA.TRANSACCION_DIARIA_PAYEE.DATE_TRANS_DIARIA >= :ARG_from )  and  ( DBA.TRANSACCION_DIARIA_PAYEE.DATE_TRANS_DIARIA <= :arg_to ) and ( DBA.TRANSACCION_DIARIA_PAYEE.ID_GROUP_TRANS_DIARIA = :arg_grupo )
UNION
  SELECT  DBA.RECEIVER.ID_RECEIVER ,                DBA.RECEIVER.NET_AMOUNT_RECEIVER ,         99999999 ,          '0010' ,          :ARG_to ,          :ARG_to ,          'AA' ,          'ENVIOS FUERA DE CLOSING'  ,           0 ,           DBA.RECEIVER.COMMISSION_PAYEE ,           'AGENT' ,          0 ,           DBA.RECEIVER.HANDLING_RECEIVER ,           DBA.RECEIVER.ID_SENDER ,           DBA.RECEIVER.ID_BRANCH ,                  DBA.RECEIVER.ID_FLAG_RECEIVER ,                     DBA.RECEIVER.ZIP_RECEIVER ,           DBA.RECEIVER.NOTES_RECEIVER ,           DBA.RECEIVER.DATE_RECEIVER ,           DBA.RECEIVER.TIME_RECEIVER ,           DBA.RECEIVER.RATE_CHANGE_RECEIVER ,           DBA.RECEIVER.TELEX_RECEIVER ,           DBA.RECEIVER.URGENCY_RECEIVER ,           DBA.RECEIVER.RECOMEND_RECEIVER ,        DBA.RECEIVER.EXCHANGE_RECEIVER ,           DBA.RECEIVER.TOTAL_RECEIVER ,             DBA.RECEIVER.TOTAL_PAY_RECEIVER ,           DBA.RECEIVER.TRANS_RECEIVER ,           DBA.RECEIVER.EXCHANGE_COMPANY ,           DBA.RECEIVER.TELEX_COMPANY ,           DBA.RECEIVER.REF_RECEIVER ,               DBA.RECEIVER.TOTAL_DIFERENCE ,           DBA.RECEIVER.TOTAL_MODO_PAGO ,           DBA.RECEIVER.TOTAL_MODO_PAGO_COMP ,               DBA.RECEIVER.STATUS_PAGO_PAYEE ,           DBA.RECEIVER.STATUS_PAGO_AGENT ,                    DBA.RECEIVER.FEE_RATE  
     FROM dba.RECEIVER,   
         dba.BRANCH   
   WHERE ( dba.BRANCH.ID_BRANCH = dba.RECEIVER.ID_BRANCH ) and  
         ( ( dba.BRANCH.ID_MAIN_BRANCH = :arg_grupo ) AND  
         ( dba.RECEIVER.id_flag_receiver not in ('A','I') ) AND  
         ( dba.RECEIVER.DATE_RECEIVER >= :arg_to ) )   order by DBA.RECEIVER.ID_RECEIVER
   " arguments=(("arg_from", date),("arg_to", date),("arg_grupo", string))  sort="id_receiver A " )
