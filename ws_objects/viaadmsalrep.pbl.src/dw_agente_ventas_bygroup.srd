 retrieve="  SELECT  DBA.AGENT.ID_AGENT ,           
  		    DBA.AGENT.NAME_AGENT ,           
  		    DBA.BRANCH_AGENT.ID_BRANCH ,           
  		    DBA.BRANCH.NAME_BRANCH ,           
  		    DBA.BRANCH.ID_STATE ,           
  		    DBA.RECEIVER.DATE_RECEIVER ,           
  		    DBA.RECEIVER.ID_RECEIVER ,           
  		    DBA.GROUP_BRANCH.ID_MAIN_BRANCH ,           
  		    DBA.GROUP_BRANCH.NAME_MAIN_BRANCH
  FROM DBA.AGENT , DBA.BRANCH , DBA.BRANCH_AGENT , DBA.RECEIVER , DBA.GROUP_BRANCH
  WHERE ( DBA.BRANCH_AGENT.ID_BRANCH = DBA.BRANCH.ID_BRANCH ) 
  AND   ( DBA.BRANCH_AGENT.ID_AGENT = DBA.AGENT.ID_AGENT ) 
  AND   ( DBA.BRANCH.ID_BRANCH = DBA.RECEIVER.ID_BRANCH ) 
  AND   ( DBA.GROUP_BRANCH.ID_MAIN_BRANCH = DBA.BRANCH.ID_MAIN_BRANCH ) 
  AND   ( ( DBA.RECEIVER.ID_FLAG_RECEIVER <> 'A' ) 
  AND     ( DATEPART(yy,DBA.RECEIVER.DATE_RECEIVER) = :YEAR1 ) )
  ORDER BY DBA.AGENT.ID_AGENT ASC,           
  		     DBA.BRANCH_AGENT.ID_BRANCH ASC,           
  		     DBA.BRANCH.ID_STATE ASC,           
  		     DBA.RECEIVER.DATE_RECEIVER ASC" arguments=(("year1", number)) )
